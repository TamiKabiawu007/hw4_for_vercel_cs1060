{
  "version": 2,
  "builds": [
    {
      "src": "api/county_data.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "includeFiles": ["data.db", "api/templates/**"]
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/county_data", "dest": "/api/county_data.py" },
    { "src": "/(.*)", "dest": "/api/county_data.py" }
  ]
}
