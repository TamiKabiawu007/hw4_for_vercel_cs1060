{
  "version": 2,
  "builds": [
    {
      "src": "api/county_data.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "50mb" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/api/static/$1"
    },
    {
      "src": "/county_data",
      "methods": ["POST"],
      "dest": "/api/county_data.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/county_data.py"
    }
  ]
}
