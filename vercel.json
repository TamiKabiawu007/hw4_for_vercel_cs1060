{
  "version": 2,
  "builds": [
    {
      "src": "api/county_data.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "50mb",
        "includeFiles": ["data.db", "api/templates/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/county_data.py"
    }
  ]
}